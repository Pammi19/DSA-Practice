class Solution {
  public:
   vector<int> findTwoElement(vector<int>& arr) {
        int n = arr.size();
         vector<int> freq(n+1, 0);
    int repeating = -1, missing = -1;

    for(int i = 0; i < n; i++) {
        freq[arr[i]]++;            //frequency count
    }

    for(int i = 1; i <= n; i++) {
        if(freq[i] == 0)
            missing = i;
        if(freq[i] > 1)
            repeating = i;
    }
    return {repeating, missing};
}
    
};
