class Solution {
  public:
    long getSecondLargest(vector<int> &arr) {
       
        long Largest = -1;
        long secondLargest = -1;
       
        for(long i : arr)
        {
            if(i>Largest)
            {
             secondLargest=Largest;
              Largest= i;
            }
       
           else if( i<Largest && i > secondLargest)
             secondLargest=i;
        }
       
         return secondLargest;
    }
};
