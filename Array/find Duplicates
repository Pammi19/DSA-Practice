class Solution {
  public:
    vector<int> findDuplicates(vector<int>& arr) {
        
    unordered_map<int, int> count;
    vector<int> duplicates;

    for (int num : arr) {
        count[num]++;
    }

    for (auto& pair : count) {
        if(pair.second > 1) {
            duplicates.push_back(pair.first);
        }
    }

    return duplicates;

    }
};
