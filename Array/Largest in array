class Solution {
  public:
    int largest(vector<int> &arr) {
        // code here
        int maxElement = arr[0];  
        
        for(int i = 1; i < arr.size(); i++) {
            if(arr[i] > maxElement) {
                maxElement = arr[i];
            }
        }
        
        return maxElement;
    }
};
T.C=O(n)
S.C=O(1)
